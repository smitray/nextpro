{
  "name": "nextpro",
  "version": "1.2.0",
  "description": "Next.js and graphQl boilerplate",
  "scripts": {
    "doc:dev": "docz dev",
    "predoc:build": "rimraf docs",
    "doc:build": "docz build",
    "dev": "backpack dev",
    "prebuild": "rimraf build",
    "build": "next build ./app/client && backpack build",
    "start": "node build/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "e2estart": "jest --config=./e2e.config.js",
    "e2e": "npm-run-all build --parallel start e2estart",
    "release": "standard-version",
    "generate": "plop --plopfile generators/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/smitray/nextpro.git"
  },
  "keywords": [
    "Next.js",
    "GraphQl",
    "Koa"
  ],
  "author": "Smit Ray <humancoder@outlook.com, hi@smitray.me, smit@codends.net, debasmit@primeinfoserv.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://gitlab.com/smitray/nextpro/issues"
  },
  "homepage": "https://gitlab.com/smitray/nextpro#readme",
  "dependencies": {
    "@hapi/joi": "^16.1.7",
    "@koa/cors": "^3.0.0",
    "@saeris/graphql-scalars": "^1.3.3",
    "apollo-boost": "^0.4.4",
    "apollo-server-koa": "^2.9.7",
    "backpack-core": "^0.8.4",
    "bcryptjs": "^2.4.3",
    "bristol": "^0.4.0",
    "cross-fetch": "^3.0.4",
    "date-fns": "^2.6.0",
    "dotenv": "^8.2.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "js-cookies": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.10.0",
    "koa-body": "^4.1.1",
    "koa-compose": "^4.1.0",
    "koa-connect": "^2.0.1",
    "koa-convert": "^1.2.0",
    "koa-helmet": "^5.2.0",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-socket-2": "^1.2.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.6",
    "mongoose-timestamp": "^0.6.0",
    "mongoose-unique-validator": "^2.0.3",
    "next": "^9.1.1",
    "next-cookies": "^1.1.3",
    "nodemailer": "^6.3.1",
    "palin": "^2.3.0",
    "polished": "^3.4.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "rimraf": "^3.0.0",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.4",
    "@chiedolabs/eslint-config-nextjs": "^0.1.3",
    "@chiedolabs/eslint-config-react": "^0.1.4",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "docz": "^2.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "gatsby-remark-copy-linked-files": "^2.1.36",
    "gatsby-remark-mermaid": "^1.2.0",
    "gatsby-remark-prismjs": "^3.3.30",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-puppeteer": "^4.4.0",
    "jest-stare": "^1.25.0",
    "jest-transform-stub": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "plop": "^2.5.3",
    "prismjs": "^1.18.0",
    "prop-types": "^15.7.2",
    "puppeteer": "^2.0.0",
    "shelljs": "^0.8.3",
    "standard-version": "^7.0.1",
    "supertest": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
